{
  "name": "web-service-typescript",
  "version": "1.0.0",
  "description": "'REST API developed with TypeScript'",
  "main": "./src/server/server.ts",
  "scripts": {
    "start": "PORT=5000 NODE_ENV=dev ./node_modules/.bin/ts-node ./src/server/server.ts",
    "builded": "PORT=5000 NODE_ENV=prod node ./dist/server/server.js",
    "QI": "tsc && NODE_ENV=prod node ./dist/server/server.js",
    "unit-test": "NODE_ENV=test mocha --config ./src/tests/unit/config/.mocharc.json ./dist/tests/unit/unit.test.js",
    "unit-cover": "nyc --reporter=html yarn integration-test",
    "integration-test": "NODE_ENV=test mocha --config ./src/tests/integration/config/.mocharc.json ./dist/tests/integration/integration.test.js",
    "integration-cover": "nyc --reporter=html yarn integration-test"
  },
  "keywords": [
    "API",
    "TypeScript",
    "Node",
    "Postgres"
  ],
  "author": "Rafael do Amaral Mario  <rafaelamaralmario@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "http": "^0.0.1-security",
    "http-status": "^1.4.2",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.19",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.0",
    "sequelize": "^6.3.3",
    "sequelize-cli": "^6.2.0",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.32",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.7",
    "@types/http-status": "^1.1.2",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.158",
    "@types/mocha": "^8.0.0",
    "@types/morgan": "^1.9.1",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "istanbul": "^0.4.5",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "supertest": "^4.0.2",
    "testdouble": "^3.16.1",
    "ts-node": "^8.10.2"
  }
}
